# Squares With Three Sides
import re

c = 0
triangles = list()
triangle  = list()

input = open('input.txt') 
data = input.read()
triangles = data.strip('\n').split('\n')

for i in range(len(triangles)):
  triangle = triangles[i]
  triangle = triangle.strip(' ')
  triangle = triangle.split(' ')
  while '' in triangle: triangle.remove('')
  triangle= list(map(int, triangle))
  triangle.sort()
  if sum(triangle[:2]) > triangle[2]:
    c = c + 1
  
print(c)
